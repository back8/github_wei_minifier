{
  "name": "compressor",
  "version": "1.0.0",
  "description": "Client-side offline-first JavaScript/CSS/HTML minifier.",
  "repository": {
    "type": "git",
    "url": "git@github.com:ddhhz/compressor.git"
  },
  "scripts": {
    "check-extraneous-pkgs": "npm list --depth=0 && echo '[SUCCESS] No extraneous packages.' || (echo '[INFO] Pruning extraneous packages...\n' && npm prune)",
    "check-eslint": "echo '\n[INFO] Running ESLint...\n' && eslint ./src/js ./*.js --ignore-path .gitignore -f table --ext .js",
    "eslint": "eslint ./src/js ./*.js --ignore-path .gitignore -f table --ext .js --fix || true",
    "build": "gulp",
    "watch": "gulp watch",
    "deploy": "cross-env NODE_ENV=production gulp && gulp deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Wei He <github@weispot.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ddhhz/compressor/issues"
  },
  "homepage": "https://compressor.whe.technology",
  "dependencies": {
    "clipboard": "^2.0.1",
    "codemirror": "^5.28.0",
    "file-saver": "^1.3.3"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "cross-env": "^5.0.3",
    "dotenv": "^5.0.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^3.7.0",
    "gulp-concat": "^2.6.1",
    "gulp-download": "0.0.1",
    "gulp-filter": "^5.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-less": "^3.3.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-pug": "^4.0.1",
    "gulp-replace": "^0.6.1",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^5.0.0",
    "gulp-webserver": "^0.9.1",
    "normalize.css": "^8.0.0",
    "pre-commit": "^1.2.2",
    "sw-precache": "^5.2.0",
    "sync-exec": "^0.6.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "pre-commit": [
    "check-extraneous-pkgs",
    "check-eslint"
  ]
}
