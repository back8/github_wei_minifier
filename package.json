{
  "name": "compressor",
  "version": "1.0.0",
  "description": "Simple web interface for compressing HTML, JS, CSS using html-minifier.",
  "repository": {
    "type": "git",
    "url": "git@github.com:ddhhz/compressor.git"
  },
  "scripts": {
    "eslint": "echo '\n[INFO] Running ESLint...\n' && eslint ./src/js ./*.js --ignore-path .gitignore -f table --ext .js --fix",
    "check-extraneous-pkgs": "npm list --depth=0 && echo '[SUCCESS] No extraneous packages.' || (echo '[INFO] Pruning extraneous packages...\n' && npm prune)",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Wei He <github@weispot.com>",
  "license": "MIT",
  "dependencies": {
    "clipboard": "^1.7.1",
    "codemirror": "^5.28.0",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.7.0",
    "gulp-concat": "^2.6.1",
    "gulp-download": "0.0.1",
    "gulp-filter": "^5.0.0",
    "gulp-flatten": "^0.3.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-less": "^3.3.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-pug": "^3.3.0",
    "gulp-replace": "^0.6.1",
    "gulp-sequence": "^0.4.6",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^4.3.11",
    "gulp-webserver": "^0.9.1",
    "normalize.css": "^7.0.0",
    "pre-commit": "^1.2.2",
    "sw-precache": "^5.2.0",
    "sync-exec": "^0.6.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "pre-commit": [
    "check-extraneous-pkgs",
    "eslint"
  ]
}
