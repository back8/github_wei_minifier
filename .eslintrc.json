{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "prefer-object-spread": 0,
    "consistent-return": 0,
    "max-len": [2, {
      "code": 120,
      "ignoreTrailingComments": true,
      "ignoreUrls": true,
      "ignoreRegExpLiterals": true,
      "ignoreTemplateLiterals": true
    }],
    "function-paren-newline": 0,
    "prefer-destructuring": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0
  }
}
